#include <stdio.h>
void main()
{
    int n,sum=0,i;
    printf("enter the number:");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        sum=sum +i;
    }
    printf("sum of the natural number:%d",sum);
}
-----------------------------------------------
Output
enter the number:5
sum of the natural number:15

