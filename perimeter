#include<stdio.h>
void main()
{
    int a,b,area,perimeter;
    printf("enter sides:\n");
    scanf("%d",&a);
    scanf("%d",&b);
    area = a*b;
    perimeter=2*(a+b);
    printf("\narea = %d",area);
    printf("\nperimeter =%d",perimeter);
}
=================================
Output
enter sides:
2
5

area = 10
perimeter =14
