#include <stdio.h>
void main()
{
    char a[100];
    char b[100];
    int i=0;
    int j=0;
    printf("enter 1 string:");
    scanf("%s",&a);
    printf("\n enter 2 string:");
    scanf("%s",&b);
    while(a[i]!='\0')
    {
        i++;
    }
    while(b[j]!='\0')
    {
        a[i]=b[j];
        i++;
        j++;
    }
    a[i]='\0';
    printf("concatenate string:%s\n",a);
}
---------------------------------------
  enter 1 string:10

 enter 2 string:20
concatenate string:1020
